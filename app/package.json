{
  "name": "expressjs-microservice-template",
  "version": "1.0.0",
  "description": "Template for microservices",
  "main": "index.js",
  "scripts": {
    "prod": "node index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "swagger-autogen": "node ./swagger.js",
    "dev": "nodemon"
  },
  "repository": {
    "type": "git",
    "url": "git+https://gitlab.com/docdz/templates/expressjs-microservice-template.git"
  },
  "keywords": [
    "microservice"
  ],
  "dependencies": {
    "bcrypt": "^5.1.0",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "express-throttle": "^2.0.0",
    "express-useragent": "^1.0.15",
    "fs": "0.0.1-security",
    "https": "^1.0.0",
    "jsonwebtoken": "^9.0.1",
    "mysql2": "^3.6.0",
    "nodemon": "^3.0.1",
    "retry": "^0.13.1",
    "sequelize": "^6.32.1",
    "stream-throttle": "^0.1.3",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0"
  },
  "author": "ALGDOC",
  "license": "ISC",
  "bugs": {
    "url": "https://gitlab.com/docdz/templates/expressjs-microservice-template/issues"
  },
  "homepage": "https://gitlab.com/docdz/templates/expressjs-microservice-template#readme",
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/preset-env": "^7.14.7",
    "babel-jest": "^27.0.6",
    "jest": "^27.0.6",
    "supertest": "^6.1.3",
    "swagger-autogen": "^2.23.5"
  },
  "jest": {
    "testEnvironment": "node",
    "testMatch": [
      "**/test/**/*.js?(x)",
      "**/?(*.)+(spec|test).js?(x)"
    ],
    "transform": {
      "^.+\\.js?$": "babel-jest"
    }
  }
}
